processor GainProcessor
{
    input  stream float in_1 [[ name: "input 1" ]];    // declare an input and output stream of floats
    output stream float out_1;

    void main()
    {
        loop   // infinite loop
        {
            out_1 <- in_1 * 0.5f + 0.1f;  // read our next input value, multiply by 0.5, and send it to our output
            advance();         // advance to the next frame
        }
    }
}
